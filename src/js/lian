        var cover = function (n) {
            var $cover=$('#cover'+n),
                pop1=$cover.find('.maskBtn').css('top'),
            pop2=$cover.find('img').css('top'),
            $cover_part=$('.cover-part'+n).offset().top;
            if(pop1) 
                pop1=pop1.replace(/[^-\d\.]/g,'');
            else pop2=0;
            if(pop2)
                pop2=pop2.replace(/[^-\d\.]/g,'');
            else pop2=0;
            var $img_$btn=pop1-pop2;
            if(n==1){
                $cover.find('.maskBtn').offset({top:$cover_part+ 'px'}).parents('div').find('.desc').offset({top:$cover_part-$img_$btn+ 'px'});
                $cover.show();
            }else if(n==3){

                var $target=$cover.find('.maskBtn'),
                    $target_margin_top=$target.css('margin-top');
                if($target_margin_top)
                    $target_margin_top=$target_margin_top.replace(/[^-\d\.]/g,'');
                else
                    $target_margin_top=0;
                $target.css('top','0px').offset({top:$cover_part-$target_margin_top});
                $cover.find('.desc').css('top','0px').offset({top:$cover_part-$img_$btn});
                $cover.show();
            }
            else if( n==2 && signed.sponsors.EnableAddress){
                $cover.find('.maskBtn').offset({top:$cover_part+ 'px'}).parents('div').find('.desc').offset({top:$cover_part-$img_$btn+ 'px'});
                $cover.show();
            }else{
                

                //$cover.show();
                //$('body').css('overflow','hidden');
            }
        }
